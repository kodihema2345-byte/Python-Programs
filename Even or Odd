def fibonacci(n):
    """Prints the Fibonacci series up to n."""
    a, b = 0, 1
    while a < n:
        print(a, end=' ') # 'end' prevents a newline after each number
        a, b = b, a + b
    print() # Prints a final newline for formatting

limit = int(input("Enter a limit for the Fibonacci series: "))
fibonacci(limit)
